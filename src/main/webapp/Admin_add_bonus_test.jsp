            <div class="fullbody">
            <div class="attendence-table">
                
                  <table id="customers">
                    <tr>
                      <th>Name</th>
                      <th>Type</th>
                      <th>Rate</th>
                      <th>Department</th>
                      <th>Monthly/Hourly</th>
                    </tr>
                    <tr>
                      <td>Overtime</td>
                      <td>Bonus</td>
                      <td><input type="text" name="OTRate" class="RateField"></td>
                      <td><select name="Department" class="RateField">
